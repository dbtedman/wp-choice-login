{
    "name": "dbtedman/wp-choice-login",
    "type": "project",
    "license": "MIT",
    "description": "Providing login choices for your WordPress site.",
    "require": {
        "php": "8.2.*|8.3.*"
    },
    "scripts": {
        "format": "phpcbf",
        "lint": "phpcs",
        "test": "phpunit"
    },
    "require-dev": {
        "mockery/mockery": "1.6.7",
        "phpunit/phpunit": "11.0.4",
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "3.9.0"
    },
    "autoload": {
        "psr-4": {
            "DBTedman\\WPChoiceLogin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DBTedman\\WPChoiceLoginTest\\": "test/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
